    "@emotion/styled": "11.1.5",
    "@nivo/core": "^0.68.0",
    "@nivo/pie": "^0.68.0",
    "@svgr/webpack": "^5.5.0",
    "@tanem/react-nprogress": "^3.0.64",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "framer-motion": "^3.5.2",
    "lodash": "^4.17.21",
    "next": "latest",
    "next-auth": "^3.29.10",
    "next-seo": "^4.24.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.3.4",
